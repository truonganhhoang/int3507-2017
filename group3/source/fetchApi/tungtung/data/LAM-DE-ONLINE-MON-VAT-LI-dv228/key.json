[{"_id": "59aaeccb4a9c7f202d2df770", "correct_answer": 1, "has_suggest_answer": true, "id": "59aaeccb4a9c7f202d2df770", "suggest_answer": {"blocks": [{"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "key": "40gji", "text": "$v=v_o+at_1=0 \\to a=-2 m/s^{2}$", "type": "unstyled"}, {"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "key": "di7v", "text": "", "type": "unstyled"}, {"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "key": "6b60d", "text": "vận tốc (sau 2s) $=v_o+at_2=10=(-2).2=6$ m/s", "type": "unstyled"}]}}, {"_id": "59aaeccb4a9c7f202d2df772", "correct_answer": 0, "has_suggest_answer": true, "id": "59aaeccb4a9c7f202d2df772", "suggest_answer": {"blocks": [{"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "key": "1hat7", "text": "Gọi $t=3s$ là thời gian vật đi hết quãng đường ", "type": "unstyled"}, {"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "key": "3gc81", "text": "$\\Rightarrow s=0,5at^{2}=4,5a$", "type": "unstyled"}, {"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "key": "e1pmt", "text": "", "type": "unstyled"}, {"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "key": "7i99l", "text": "Thời gian vật đi quãng đường (1/9)s là: ", "type": "unstyled"}, {"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "key": "6d3l9", "text": "$(1/9s)=4,5a/9=0,5a=0,5at_1^{2}  => t_1=1s$", "type": "unstyled"}, {"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "key": "11ld2", "text": "", "type": "unstyled"}, {"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "key": "8eitl", "text": "$\\Rightarrow$ thời gian vật đi 8/9 quãng đường cuối $t_2=t-t_1=2s$                                                 ", "type": "unstyled"}]}}, {"_id": "59aaeccb4a9c7f202d2df774", "correct_answer": 0, "has_suggest_answer": false, "id": "59aaeccb4a9c7f202d2df774", "suggest_answer": ""}, {"_id": "59aaeccb4a9c7f202d2df776", "correct_answer": 2, "has_suggest_answer": true, "id": "59aaeccb4a9c7f202d2df776", "suggest_answer": {"blocks": [{"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "key": "uq08", "text": " Một vật nhiễm điện do tiếp xúc thì đã có sự di chuyển điện tích giữa các vật. \n ", "type": "unstyled"}, {"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "key": "7hhjk", "text": "Xét về toàn bộ một vật nhiễm điện do tiếp xúc vật sẽ không trung hòa về điện.\n", "type": "unstyled"}, {"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "key": "59ear", "text": "Vật nhiễm điện do hưởng ứng thì chỉ có sự sắp xếp lại vị trí các điện tích trong vật mà không có sự di chuyển điện tích ra ngoài vật nên về tổng thể thì vật vẫn là một vật trung hòa điện. ", "type": "unstyled"}]}}, {"_id": "59aaeccb4a9c7f202d2df778", "correct_answer": 1, "has_suggest_answer": true, "id": "59aaeccb4a9c7f202d2df778", "suggest_answer": {"blocks": [{"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "key": "caamp", "text": " Nhiễm điện do hưởng ứng xảy ra với một vật tich điện đặt gần một vật dẫn điện.\n", "type": "unstyled"}, {"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "key": "9g0lq", "text": "Nhựa không phải vật dẫn điện nên trường hợp đặt quả cầu mang điện gần thanh nhựa sẽ không xảy ra hiện tượng hưởng ứng. ", "type": "unstyled"}]}}, {"_id": "59aaeccb4a9c7f202d2df77a", "correct_answer": 0, "has_suggest_answer": true, "id": "59aaeccb4a9c7f202d2df77a", "suggest_answer": {"blocks": [{"depth": 0, "entityRanges": [], "inlineStyleRanges": [{"length": 79, "offset": 0, "style": "BOLD"}], "key": "3q6q4", "text": "để 1 vật A nhiễm điện cho vật B C ko nhiễm điện để điện tích của B C khác dấu. ", "type": "unstyled"}, {"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "key": "41n3q", "text": "", "type": "unstyled"}, {"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "key": "adcek", "text": "đáp án  B: nhiễm điện do tiếp xúc, vật cùng dấu vs vật mang điện tích -> A B C cùng dấu. ", "type": "unstyled"}, {"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "ke