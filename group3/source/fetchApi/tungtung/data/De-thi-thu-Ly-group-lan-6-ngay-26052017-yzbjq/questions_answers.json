[{"__v": 0, "_id": "5927c132be43467abbd18442", "answers": [{"answer": "Tần số góc của dao động điều hòa bằng tần số góc của chuyển tròn đều.", "id": "4b454", "type": "editor"}, {"answer": "Biên độ của DĐĐH bằng bán kín của chuyển động tròn đều.", "id": "bt5UG", "type": "editor"}, {"answer": {"blocks": [{"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "key": "786so", "text": "Lực kéo về trong DĐĐH có độ lớn bằng độ lớn lực hướng tâm trong chuyển động tròn đều.", "type": "unstyled"}]}, "id": "4VmkI", "type": "editor"}, {"answer": {"blocks": [{"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "key": "ar16e", "text": "Tốc độ cực đại của DĐĐH bằng tốc độ dài của chuyển động tròn đều.", "type": "unstyled"}]}, "id": "xRtGp", "type": "editor"}], "created_at": "2017-05-26T05:46:26.641Z", "has_suggest_answer": false, "id": "5927c132be43467abbd18442", "question": {"blocks": [{"depth": 0, "entityRanges": [], "inlineStyleRanges": [{"length": 3, "offset": 139, "style": "BOLD"}], "key": "s0t4", "text": "Hình chiếu của một chất điểm chuyển động tròn đều lên một đường kính quỹ đạo có chuyển động là dao động điều hòa. Phát biểu nào sau đây là sai ?", "type": "unstyled"}]}, "type": "multiple_choose", "updated_at": "2017-05-26T15:49:38.466Z"}, {"__v": 0, "_id": "5927c132be43467abbd18444", "answers": [{"answer": "cường độ âm", "id": "R03V7", "type": "editor"}, {"answer": {"blocks": [{"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "key": "d64jk", "text": "độ to", "type": "unstyled"}]}, "id": "MJZEP", "type": "editor"}, {"answer": "đồ thị li độ âm", "id": "ovk7H", "type": "editor"}, {"answer": {"blocks": [{"depth": 0, "entityRanges": [], "inlineStyleRanges": [], "key": "25uu0", "text": "mức cường độ âm", "type": "unstyled"}]}, "id": "0Ebx4", "type": "editor"}], "created_at": "2017-05-26T05:46:26.677Z", "has_suggest_answer": false, "id": "5927c132be43467abbd18444", "question": "Hãy cho biết đâu là đặc tính sinh lý của âm ?", "type": "multiple_choose", "updated_at"